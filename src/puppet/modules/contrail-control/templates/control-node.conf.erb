[DEFAULT]
# bgp_config_file=bgp_config.xml
# bgp_port=179
# collectors= # Provided by discovery server
  hostip=<%= host_ip %> # Resolved IP of `hostname`
  hostname=<%= hostname %> # Retrieved as `hostname`
# http_server_port=8083
# log_category=
# log_disable=0
  log_file=/var/log/contrail/control-node.log
# log_files_count=10
# log_file_size=10485760 # 10MB
# log_level=SYS_NOTICE
# log_local=0
# test_mode=0
# xmpp_server_port=5269

[DISCOVERY]
# port=5998
  server=<%= contrail_discovery_ip %> # discovery-server IP address

[IFMAP]
  certs_store=
  password=<%= contrail_config_passwd %>
# server_url= # Provided by discovery server, e.g. https://127.0.0.1:8443
  user=<%= contrail_config_user %>

